{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "sourceMap": false,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "outDir": "../build",
    "rootDir": "./",
    "strictPropertyInitialization": false,
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
